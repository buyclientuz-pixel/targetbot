{
  "name": "targetbot",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx wrangler@4.46.0 dev",
    "deploy": "npm run lint:api && npx wrangler@4.46.0 deploy",
    "build": "npx wrangler@4.46.0 deploy --dry-run",
    "lint": "npx eslint . --ext .ts,.js",
  "lint:api": "npx -y tsc --noEmit",
    "check:config": "node scripts/check-config.mjs",
    "sync:secrets": "node scripts/sync-secrets.mjs",
    "test:integration": "node scripts/run-integration.mjs",
    "validate:postdeploy": "node scripts/postdeploy-validate.mjs"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@cloudflare/workers-types": "^4.2025.0"
  }
}
