{
  "name": "targetbot",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "node ./scripts/ensure-registry.mjs",
    "build": "wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "lint": "eslint src tests scripts --ext .js,.ts",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "test": "node --loader ./scripts/ts-loader.mjs --test \"tests/**/*.test.ts\"",
    "qa": "node --loader ./scripts/ts-loader.mjs --test \"tests/**/*.test.ts\"",
    "dry-run": "node ./scripts/dry-run.mjs",
    "format": "prettier \"**/*.{ts,js,json,md}\"",
    "sync:secrets": "node ./scripts/sync-secrets.mjs"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.15.0",
    "wrangler": "^4.47.0"
  }
}
